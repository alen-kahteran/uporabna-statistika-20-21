Cell[TextData[{
 "Ukaz ",
 StyleBox["Expand",
  FontFamily->"Courier New",
  FontWeight->"Bold"],
 StyleBox["[ ]",
  FontWeight->"Bold"],
 " raz\[SHacek]iri produkt faktorjev v vsoto potenc.\nUkaz ",
 StyleBox["Factor[ ]",
  FontWeight->"Bold"],
 " razcepi dani polinom na produkt kvadratnih in linearnih faktorjev.\nUkaz \
",
 StyleBox["Together",
  FontFamily->"Courier New",
  FontWeight->"Bold"],
 StyleBox["[ ] ",
  FontWeight->"Bold"],
 "postavi ulomke na skupni imenovalec.\nUkaza ",
 StyleBox["Numerator[ ] ",
  FontWeight->"Bold"],
 "in ",
 StyleBox["Denominator[ ]",
  FontWeight->"Bold"],
 " vrneta \[SHacek]tevec oz. imenovalec danega ulomka.\nUkaz ",
 StyleBox["PolynomialQuotient[ ]",
  FontWeight->"Bold"],
 " vrne kvocient dveh polinomov.\n\nOpazimo, da ",
 StyleBox["Mathematica",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox[" ",
  FontWeight->"Bold"],
 "vse rezultate \[SHacek]tevil\[CHacek]i.\nIzraz ",
 StyleBox["%",
  FontWeight->"Bold"],
 " ozna\[CHacek]uje zadnji izra\[CHacek]unani ",
 StyleBox["Out",
  FontWeight->"Bold"],
 " izraz. Podobno, ",
 StyleBox["%%",
  FontWeight->"Bold"],
 " ozna\[CHacek]uje predzadnji izra\[CHacek]unani rezultat. \n",
 StyleBox["%10",
  FontWeight->"Bold"],
 " predstavlja izraz ",
 StyleBox["Out[10]",
  FontWeight->"Bold"],
 ". Z uporabo teh oznak se lahko sklicujemo na dolo\[CHacek]ene rezultate.\n\
",
 StyleBox["Pozor",
  FontWeight->"Bold"],
 " na dejstvo, da zadnji rezultat ni ve\[CHacek] zadnji, ko \
izra\[CHacek]unamo naslednjega!"
}], "Text",
